setenv PAGER more
setenv EDITOR vi

# set default home dir to /home/nrts
# CHANGE AS NEEDED IF NRTS USER HOME DIR WILL BE ELSEWHERE 
setenv NRTS_HOME /home/nrts

setenv IDA_DBDIR  $NRTS_HOME
setenv Q330_ROOT  $NRTS_HOME/etc
setenv DBIO_DATABASE $NRTS_HOME
setenv BLOCKSIZE 1024
setenv BOOT_BUILD_FLAG $NRTS_HOME/reconfigure

# serial port identifier for Q330 serial connection with station computer.
# system dependent
setenv Q330_CONSOLE /dev/cuau1
